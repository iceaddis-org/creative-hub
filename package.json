    "lucide-react": "^0.378.0",
    "next": "15.2.2",
    "next-sitemap": "^4.2.3",
    "payload": "3.31.0",
    "pnpm": "^10.12.1",
    "prism-react-renderer": "^2.3.1",
    "react": "19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
